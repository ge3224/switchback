{
  "name": "switchback",
  "version": "0.1.0",
  "description": "Single-page apps with your preferred server stack - zero dependencies, vendorable, auditable",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "VITE_ROOT=examples vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "keywords": [
    "spa",
    "inertia",
    "navigation",
    "vanilla",
    "typescript",
    "zero-dependency",
    "vendorable",
    "auditable"
  ],
  "author": {
    "name": "Jacob Benison",
    "url": "https://jacobbenison.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ge3224/switchback.git"
  },
  "bugs": {
    "url": "https://github.com/ge3224/switchback/issues"
  },
  "homepage": "https://github.com/ge3224/switchback#readme",
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^25.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@9.0.0"
}
